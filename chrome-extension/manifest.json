{
    "manifest_version": 3,
    "name": "Discord Content Toggle",
    "description": "Adds the ability to toggle the visibility of content in Discord chats.",
    "version": "1.0",
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*.discord.com/*"],
          "js": ["inject.js"]
        }
      ]
  }